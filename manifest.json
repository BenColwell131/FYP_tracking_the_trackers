{
  "name": "Tracking the Trackers",
  "version": "1.0",
  "description": "Monitor WebSocket Activity",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
     "scripts": ["background.js"],
     "persistent": false
   },
   "browser_action": {
     "default_popup": "popup.html",
     "default_icon": {
       "16": "images/ttt_v2_16.png",
       "32": "images/ttt_v2_32.png",
       "48": "images/ttt_v2_48.png",
       "128": "images/ttt_v2_128.png"
     }
   },
   "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "run_at": "document_start",
       "js": ["contentScript.js"]
     }
   ],
   "web_accessible_resources": ["webSocketPatch.js"],
   "options_page": "options.html",
   "icons": {
      "16": "images/ttt_v2_16.png",
      "32": "images/ttt_v2_32.png",
      "48": "images/ttt_v2_48.png",
      "128": "images/ttt_v2_128.png"
    },
  "manifest_version": 2
}
